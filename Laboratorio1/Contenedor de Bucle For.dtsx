        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            // TODO: Add your code here
            try
            {
                string NombreExcel2 = Dts.Variables["User::NombreExcel2"].Value.ToString();
                string NombreHoja = Dts.Variables["User::NombreHoja"].Value.ToString();
                string RutaCarpeta = Dts.Variables["User::RutaCarpeta"].Value.ToString();
                string TablaProducts = Dts.Variables["User::TablaProducts"].Value.ToString();
                string TablaCategories = Dts.Variables["User::TablaCategories"].Value.ToString();

                OleDbConnection Excel_OLE_Con = new OleDbConnection();
                OleDbCommand Excel_OLE_Cmd = new OleDbCommand();

                string connString = "Provider=Microsoft.ACE.OLEDB.12.0;" + "Data Source=" + RutaCarpeta + NombreExcel2 +
                    "; Extended Properties=\"Excel 12.0 Xml;HDR=YES;\"";

                File.Delete(RutaCarpeta + "\\" + NombreExcel2 + ".xlsx");

                SqlConnection myADONETConnection = new SqlConnection();
                myADONETConnection = (SqlConnection)(Dts.Connections["DB_Conn_NORTHWND"].AcquireConnection(Dts.Transaction) as SqlConnection);

                string queryString = "SELECT P.ProductID, p.ProductName, p.CategoryID, c.CategoryName, " +
                    "p.QuantityPerUnit, p.UnitPrice, p.UnitsInStock FROM" + TablaProducts + "AS P INNER JOIN " +
                    TablaCategories + " AS C On P.CategoryID = C.CategoryID";

                SqlDataAdapter adapter = new SqlDataAdapter(queryString, myADONETConnection);
                DataSet ds = new DataSet();
                adapter.Fill(ds);

                String TableColumns = string.Empty;

                foreach (DataTable table in ds.Tables)
                {
                    foreach (DataColumn column in table.Columns)
                    {
                        TableColumns += column + "],[";
                    }
                }

                TableColumns = ("[" + TableColumns.Replace(",", " Text,").ToString());
                TableColumns = TableColumns.Remove(TableColumns.Length - 2);

                Excel_OLE_Con.ConnectionString = connString;
                Excel_OLE_Con.Open();
                Excel_OLE_Cmd.Connection = Excel_OLE_Con;
                Excel_OLE_Cmd.CommandText = "CREATE TABLE " + NombreHoja + " (" + TableColumns + ")";
                Excel_OLE_Cmd.ExecuteNonQuery();

                Excel_OLE_Con.Close();

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                using (StreamWriter sw = File.CreateText(Dts.Variables["User::RutaCarpeta"].Value.ToString() + "\\" +
                    Dts.Variables["User::NombreExcel2"].Value.ToString() + ".log"))
                {
                    sw.WriteLine(ex.ToString());
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
            }

            Dts.TaskResult = (int)ScriptResults.Success;
		}

